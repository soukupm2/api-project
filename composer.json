{
	"name": "soukupm2/api-project",
	"description": "Api project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["GPL-3.0-only"],
	"require": {
		"php": ">= 8.2",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"league/openapi-psr7-validator": "^0.22.0",
		"guzzlehttp/psr7": "^2.7",
		"ramsey/uuid": "^4.7"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"nette/tester": "^2.5",
		"symplify/easy-coding-standard": "^12.5",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-doctrine": "^2.0",
		"phpstan/phpstan-nette": "^2.0"
	},
	"autoload": {
